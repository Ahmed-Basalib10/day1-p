1.
#include<stdio.h>
struct sum
{
   int a[100];
   int b;
   int add;
};


struct sum input()
{
   struct sum i;
   printf("How manyn elements you want to add?\n");
   scanf("%d",&i.b);
   printf("Enter elements:\n");
   for(int k=0;k<i.b;k++)
   {
       scanf("%d",&i.a[k]);
   }
   return i;
}

struct sum compute()
{
   struct sum i;
   i.add = 0;
   for(int k = 0; k<i.b;k++)
   {
       i.add = i.add + i.a[k];
   }
   return i;
}

struct sum output()
{
   struct sum i;
   printf("Sum of given numbers is %d\n",i.add);
}

int main()
{
   struct sum i;
   input();
   compute();
   output();
   return 0;
}


